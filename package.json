{
  "name": "discord-rankup",
  "version": "1.0.0",
  "description": "A simple discord.js xp and level system",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://discord-rankup.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/x404dev/discord-rankup"
  },
  "bugs": {
    "url": "https://github.com/x404dev/discord-rankup/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "docs": "npx typedoc --entryPointStrategy expand ./src --exclude '**/*+.test.ts' --json .tmp/typedoc-out.json && ts-node ./docs",
    "lint": "eslint ./src --ext .ts --ignore-pattern **/*.test.ts",
    "lint:fix": "eslint ./src --ext .ts --fix --ignore-pattern **/*.test.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "preversion": "npm run lint && npm run build && npm run test",
    "version": "npm run format && git add .",
    "postversion": "git push && git push --tags"
  },
  "author": "x404dev",
  "license": "ISC",
  "keywords": [
    "discord",
    "discord.js",
    "ranks",
    "levels",
    "xp"
  ],
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "ts-docgen": "github:dbots-pkg/ts-docgen#main",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jsdoc-babel": "^0.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "discord.js": "^14.8.0",
    "mongoose": "^7.0.1"
  }
}
